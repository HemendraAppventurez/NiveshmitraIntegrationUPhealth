@{
    ViewBag.Title = "Immunization Certificate DashBoard";
    Layout = "~/Views/Shared/_UserLayout.cshtml";
}

<div class="content-title">
    <h3 class="main-title">
        <strong>टीकाकरण प्रमाणपत्र  डैशबोर्ड <span class="small">Immunization Certificate Dashboard</span></strong>
    </h3>
</div>

<div id="content" class="dashboard padding-90 margin-bottom-50">
    <div class="row">
        <div class="col-md-12">
            <ul class="circle">
                <li class="col-md-2 col-sm-6 col-xs-12"></li>
                <li class="col-md-4 col-sm-6 col-xs-12">
                    <a href="@Url.Action("RegistrationInstructions", "IMC")" class="red">
                        <img src="@Url.Content("~/images/womens-health.png")"><br />                        
                        New Registration Application Form
                        @*<span class="nmicon2">Print Your Application Form</span>*@
                    </a>
                </li>
                <li class="col-md-4 col-sm-6 col-xs-12">
                    <a href="@Url.Action("ViewIMCList", "IMC")" class="indigo">
                       <img src="@Url.Content("~/images/view_icon.png")"><br />
                        View Application Form Status
                        @*<span class="nmicon2">View Application Status</span>*@
                    </a>
                </li>
            </ul>
        </div>
    </div>
</div>

