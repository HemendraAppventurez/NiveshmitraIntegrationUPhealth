@{
    ViewBag.Title = "DICDashBoard";
    Layout = "~/Views/Shared/_UserLayout.cshtml";
}

<div class="content-title">
    <h3 class="main-title">
        <strong>विकलांगता प्रमाणपत्र डैशबोर्ड<span class="small">Disability Certificate  Dashboard</span></strong>
