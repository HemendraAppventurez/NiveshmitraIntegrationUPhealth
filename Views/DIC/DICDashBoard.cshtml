                    <a href="@Url.Action("PrintApplicationForm", "DIC", new {registrationNo=ViewBag.RegisNo })" class="red" target="_blank">
                        <img src="@Url.Content("~/images/print_icon.png")"><br />
                        Print of Submitted Application
                    </a>
                </li> *@
                <li class="col-md-4 col-sm-6 col-xs-12">
                    <a href="@Url.Action("ViewDICList", "DIC")" class="indigo">
                         <img src="@Url.Content("~/images/view_icon.png")"><br />
                        Status of Submitted Application
                    </a>
                </li>
                <li class="col-md-4 col-sm-6 col-xs-12">
                    <a href="@Url.Action("DisabilityCertificate", "DIC",new {RegType=@OTPL_Imp.CustomCryptography.Encrypt("renew") })" class="red">
