    <div class="clearfix"></div>
</div>
<script>
    $(document).ready(function () {
        ChildListPartner();
        DOClist();
        ChildList();
    });
    function ChildListPartner() {
        debugger;
        $.ajax({
            url: '@Url.Action("BindChildPartner", "NUH")',
            datatype: 'html',
            type: 'GET',
            async: false,
            success: function (data) {
                $("#childlistPartner").html(data);
            },
        });
    }
    function DOClist() {
        debugger;
        $.ajax({
            url: '@Url.Action("BindDOCList", "NUH")',
            datatype: 'html',
            type: 'GET',
            async: false,
            success: function (data) {
                $("#childlistDOC").html(data);
            },
        });
    }
    function ChildList() {
        debugger;
        $.ajax({
            url: '@Url.Action("BindChildList", "NUH")',
            datatype: 'html',
            type: 'GET',
            async: false,
            success: function (data) {
                $("#childlist").html(data);
            },
        });
    }

</script>
