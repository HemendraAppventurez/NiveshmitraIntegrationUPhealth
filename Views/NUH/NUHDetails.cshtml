@model CCSHealthFamilyWelfareDept.Models.NUHmodel

@{
    ViewBag.Title = "NUHDetails";
    Layout = "~/Views/Shared/_UserLayout.cshtml";
    int io = @ViewBag.VBoutpateint.Count;
    int il = @ViewBag.VBolaboratory.Count;
    int ii = @ViewBag.VBimaging.Count;
    
}
<style>
    .stronglbl label {
        font-weight: 600 !important;
        display: block;
    }

    .form-group {
        border-bottom: 1px solid #ccc;
    }

        .form-group:last-child {
            border-bottom: 0px solid #ccc;
        }

    .hed {
        font-weight: bold;
        margin: 0 0 8px;
        padding: 0;
    }

    #childlistPartner h3, #childlistDOC h3, #childlist h3 {
        border-bottom: 1px solid #e4e4e4 !important;
        padding-bottom: 10px !important;
        font-size: 17px !important;
        color: #0dc0d8 !important;
        padding-top: 0 !important;
        margin-top: 0 !important;
    }

        #childlistPartner h3 span, #childlistDOC h3 span, #childlist h3 span {
            padding: 0 !important;
            margin: 0 !important;
        }
</style>

<div class="content-title">
    <h3 class="main-title">Medical Establishment Details       
        <a href="@Url.Action("ViewNUHList", "NUH")" class="btn btn-info btn-md pull-right">Back</a>
    </h3>
</div>
<div id="content" class="dashboard padding-20 margin-bottom-50">
