            {
                <div class="form-group">

                    <div class="col-md-3">
                        <label>Certificate from agency to Disposal of Medical Waste</label>
                    </div>
                    <div class="col-md-3">
                        <label>Yes</label>
                    </div>

                    <div class="col-md-3">
