                    </div>
                    <div class="clearfix"></div>
                </div>
                <div class="form-group">

                    <div class="col-md-3">
                        <label>Old Certificate</label>
                    </div>
                    <div class="col-md-3">
                        @if (!string.IsNullOrEmpty(Model.outerCertificateFile))
                        {
                            <a href="@Url.Content(@Model.outerCertificateFile)" target="_blank"><i class="fa fa-download fa-2x"></i></a>  
                        }
                    </div>
                    <div class="col-md-3">
                        <label></label>
                    </div>
                    <div class="col-md-3">
                    </div>
                    <div class="clearfix"></div>
                </div>
