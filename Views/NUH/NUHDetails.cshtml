                    </div>
                    <div class="col-md-3">
                        <label>Other OutPatient</label>
                    </div>
                    <div class="col-md-3">
                        @Model.otherOutPatient
                    </div>



                    <div class="clearfix"></div>
                </div>
                <div class="form-group">
                    <div class="col-md-3">
                        <label>OutPatient type </label>
                    </div>
                    <div class="col-md-6">


                        @foreach (var v in ViewBag.VBoutpateint)
                        {                    
              
                            <span>@v.outPatientName
                                @if (io > 1)
                                { 
                                    <span>,</span>
                                }

                            </span>
                                io--;
                        }
                    </div>
                </div>
            }
            else
            {
                <div class="form-group">

                    <div class="col-md-3">
                        <label>OutPatient</label>
                    </div>
                    <div class="col-md-3">
                        <label>No</label>
                    </div>
                    <div class="clearfix"></div>
                </div>
            }
            @if (Model.isLaboratory == true)
            {
                <div class="form-group">

                    <div class="col-md-3">
                        <label>Laboratory</label>
                    </div>
                    <div class="col-md-3">
                        <label>Yes</label>
                    </div>
                    <div class="col-md-3">
                        <label>Other Laboratory</label>
                    </div>
                    <div class="col-md-3">
                        @Model.otherLaboratory
                    </div>



                    <div class="clearfix"></div>
                </div>
                <div class="form-group">
                    <div class="col-md-3">
                        <label>Laboratory  type </label>
                    </div>
                    <div class="col-md-6">


                        @foreach (var v in ViewBag.VBolaboratory)
                        {                    
              
                            <span>@v.laboratoryName
                                @if (il > 1)
                                { 
                                    <span>,</span>
                                }

                            </span>
                                il--;
                        }
                    </div>
                </div>
            }
            else
            {
