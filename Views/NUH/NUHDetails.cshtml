            }
            <h4 class="hed">Details of Person In Charge</h4>
            <div class="form-group">
                <div class="col-md-3">
                    <label>Name</label>
                </div>
                <div class="col-md-3">
                    @Model.applicantName
                </div>
                <div class="col-md-3">
                    <label>Mobile No</label>
                </div>
                <div class="col-md-3">
                    @Model.applicantMobileNo
                </div>
                <div class="clearfix"></div>
            </div>
            <div class="form-group">
                <div class="col-md-3">
                    <label>Email Id</label>
                </div>
                <div class="col-md-3">
                    @Model.applicantEmailId
                </div>
                <div class="col-md-3">
                    <label>Relevant Qualification</label>
                </div>
                <div class="col-md-3">
                    @Model.qualification
                </div>

                <div class="clearfix"></div>
            </div>
            <div class="form-group">
                <div class="col-md-3">
                    <label>Institution</label>
