                    </div>
                    <div class="col-md-3">
                        @Model.nocCertificationNo
                    </div>
                    <div class="clearfix"></div>
                </div>
                <div class="form-group">

                    <div class="col-md-3">
                        <label>NOC from Pollution control Board Certificate</label>
                    </div>
                    <div class="col-md-3">
                        @if (!string.IsNullOrEmpty(Model.nOCFilePath))
                        {
                            <a href="@Url.Content(@Model.nOCFilePath)" target="_blank"><i class="fa fa-download fa-2x"></i></a>
                        }
                    </div>


