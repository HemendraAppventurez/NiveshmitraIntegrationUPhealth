                </div>
                <div class="col-md-3">
                    @Model.registrationNumber
                </div>
                <div class="col-md-3">
                    <label>Address</label>
                </div>
                <div class="col-md-3">
                    @Model.applicantAddress
                </div>

                <div class="clearfix"></div>
            </div>
            <div class="clearfix"></div>
            <div class="form-group">
                <div class="col-md-3">
                    <label>State</label>
                </div>
                <div class="col-md-3">
                    @Model.applicantStateName
                </div>
                <div class="col-md-3">
                    <label>District</label>
                </div>
                <div class="col-md-3">
                    @Model.applicantDistrictName
                </div>

                <div class="clearfix"></div>
            </div>
            <div class="clearfix"></div>
            <div class="form-group">

                <div class="col-md-3">
                    <label>Pincode</label>
                </div>
                <div class="col-md-3">
                    @Model.pinCode
                </div>
                <div class="col-md-3">
                    <label>Photo</label>
                </div>
                <div class="col-md-3">
                    @if (!string.IsNullOrEmpty(Model.piPhotograph))
                    {
                        <a href="@Url.Content(@Model.piPhotograph)" target="_blank"><i class="fa fa-download fa-2x"></i></a>
                    }
                </div>



                <div class="clearfix"></div>
            </div>

            <div class="clearfix"></div>
            <div class="form-group">

                <div class="col-md-3">
                    <label>Upload MCI/SMF Certificate</label>
                </div>
                <div class="col-md-3">
                    @if (!string.IsNullOrEmpty(Model.upmci_smfCertificateFilePath))
                    {
                        <a href="@Url.Content(@Model.upmci_smfCertificateFilePath)" target="_blank"><i class="fa fa-download fa-2x"></i></a>
                    }
                </div>

                <div class="clearfix"></div>
            </div>
            <div class="clearfix"></div>

            <div id="childlistDOC">
            </div>
            <div id="childlist">
            </div>
            <h4 class="hed">Type of Facilities Offered</h4>
            @if (Model.isInPatient == true)
            {
                <div class="form-group">

                    <div class="col-md-3">
                        <label>InPatient</label>
                    </div>
                    <div class="col-md-3">
                        <label>Yes</label>
                    </div>
                    <div class="col-md-3">
                        <label>No. of Bed</label>
                    </div>
                    <div class="col-md-3">
                        @Model.numberofBed
                    </div>



                    <div class="clearfix"></div>
                </div>
                
            }
            else
            {
                <div class="form-group">

                    <div class="col-md-3">
                        <label>InPatient</label>
                    </div>
                    <div class="col-md-3">
                        <label>No</label>
                    </div>
                    <div class="clearfix"></div>
                </div>
            }
            @if (Model.isOutPatient == true)
            {
                <div class="form-group">

                    <div class="col-md-3">
                        <label>OutPatient</label>
                    </div>
                    <div class="col-md-3">
