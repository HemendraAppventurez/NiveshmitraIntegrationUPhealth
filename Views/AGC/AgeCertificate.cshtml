            isShowPopup = false;
        }

        if (isShowPopup) {

            $('#spanApplicantType').text($('input[name=applicantTypeId]:checked').parent().text());
            $('#lblViewReqByType').text($('input[name=applicantTypeId]:checked').parent().text());
            
            if ($('input[name=applicantTypeId]:checked').parent().text().trim() == 'Other') {
                $('#spanSubApplicantType').text($('#applicantSubTypeOther').val());
            }
            else {
                $('#spanSubApplicantType').text($('#applicantSubTypeId option:selected').text());
            }
            
            $('#spanOrderDetails').text($('#orderDetail').val());
