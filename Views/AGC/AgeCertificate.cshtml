    var strDate = @DateTime.Now.Year + "/" + @DateTime.Now.Month + "/" + @DateTime.Now.Day + "";
     
    $('#orderDate').datepicker({
        maxDate: new Date(strDate),
        autoClose: true
    });

    $('#appointmentDate').datepicker({
        minDate: new Date(strDate),
