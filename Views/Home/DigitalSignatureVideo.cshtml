@{
    ViewBag.Title = "DigitalSignatureVideo";
    Layout = null;
}
<html class=" gecko win js" lang="en">
<head>
    <meta http-equiv="content-type" content="text/html; charset=UTF-8">
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>@ViewBag.Title Health and Family Welfare Department Uttar Pradesh</title>
    <link href="~/css/bootstrap.min.css" rel="stylesheet" type="text/css" />
    <link href="~/css/app.css" rel="stylesheet" type="text/css" />
    <link href="~/css/essentials.css" rel="stylesheet" type="text/css" />
    <link href="~/css/extra.css" rel="stylesheet" type="text/css" />
    <link href="~/css/font-awesome.min.css" rel="stylesheet" type="text/css" />

    <script type="text/javascript" src="~/js/jquery-1.11.3.min.js"></script>
    <script type="text/javascript" src="~/js/bootstrap.min.js"></script>
</head>
<body class="admin">
    <div id="app-wrapper" class="app-hide-lpanel">
        <div id="app-container">
            <section id="main-content" style="margin-left: 0 !important;">
                <header id="app-header">
                    <div class="app-right-header">
                        <span class="deptname" style="margin-left: 15px !important;">
                            <img src="~/images/logo.png" alt="" class="img-responsive" /></span>                       
                    </div>
                </header>
                <div class="content-title">
                    <h3 class="main-title">
                        <div class="col-md-12"><strong>डिजिटल सिग्नेचर वीडियो / <span class="small" style="display: inline-block !important;">Digital Signature Video</span></strong></div>
                    </h3>
                </div>

                <div id="content" class="dashboard padding-20 margin-bottom-50">
                    <div class="row">
                        <div class="col-md-8 col-md-offset-2">
                            @*<video src="~/Content/DigitalSignAppDoc/Digital%20SIgnature%2013.mp4" controls="controls" style="width:100%; height:450px; margin:0 auto;" />*@
                            <iframe width="100%" height="450" src="https://www.youtube.com/embed/8ZmrXGem5F4" frameborder="1" allow="accelerometer; autoplay; encrypted-media; gyroscope; picture-in-picture" allowfullscreen></iframe>
                        </div>
                    </div>
                </div>
                <div class="clearfix"></div>
            </section>
        </div>
    </div>
</body>
</html>
