        return false;
    return true;
}
function isalphabet(event) {
    debugger;
    var charCode = (event.which) ? event.which : event.keyCode

    if ((charCode >= 65 && charCode <= 91) || charCode == 8 || charCode == 32 ||
         (charCode >= 97 && charCode <= 122) || charCode == 46)
        return true;
    else
        return false;
}

