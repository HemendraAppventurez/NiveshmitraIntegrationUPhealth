function DeleteRow(obj) {

    if ($("#step").val() > 2) {
        rowCount = $(".tbUser td").closest("tr").length - 1;
    }
    if (rowCount > 0) {

        swal({
            title: "Are you sure?",
            type: "warning",
            showCancelButton: true,
            confirmButtonClass: "btn-danger",
            confirmButtonText: "Yes, Delete it !",
