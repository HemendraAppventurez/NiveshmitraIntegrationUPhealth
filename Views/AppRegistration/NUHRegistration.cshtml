            closeOnConfirm: true,
            closeOnCancel: true
        }, function (isConfirm) {
            if (isConfirm) {
                $(obj).closest('tr').remove();
                rowCount--;
            }
        });
    }
    else {
        swal("", "Details can not be deleted ", "warning");
    }
}
