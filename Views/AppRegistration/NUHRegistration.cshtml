@model CCSHealthFamilyWelfareDept.Models.NUHmodel

@{
    ViewBag.Title = "NUHRegistration";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
    int z = 0;
}
<style>
    .css-serial {
        counter-reset: serial-number; /* Set the serial number counter to 0 */
    }

        .css-serial td:first-child:before {
            counter-increment: serial-number; /* Increment the serial number counter */
            content: counter(serial-number); /* Display the counter */
        }

    .table-bordered {
        border: 2px solid #ddd;
    }
</style>


<div class="content-title">
    <h3 class="main-title">@ViewBag.Toptitle  <strong>Medical Establishment</strong></h3>

</div>

@using (Html.BeginForm("NUHRegistration", "AppRegistration", FormMethod.Post, new { @id = "frmData" }))
{
    @Html.AntiForgeryToken()
    @Html.HiddenFor(m => m.step)
    @Html.HiddenFor(m => m.regisIdNUH)
    @Html.HiddenFor(m => m.stepValue)
    @Html.HiddenFor(m => m.isCertificateFromPortal)
    @Html.HiddenFor(m => m.isRenewal)
    <div id="content" class="dashboard padding-20 margin-bottom-50">
        @if (Model.step == 0 || Model.stepValue == 0)
        { 
            <div class="row">
                <div class="col-md-12">
                    <div class="panel panel-default">
                        <div class="panel-heading">
                            <span class="title elipsis">
                                <h4>Step 1</h4>
                            </span>
                        </div>
                    </div>
                </div>
                <div class="col-md-12 oldrenewal" style="display: none">
                    <div class="panel panel-default">
                        <div class="panel-heading">
                            <span class="title elipsis">
                                <h4>Details of Previous Certificate</h4>
                            </span>
                        </div>
                        <div class="panel-body">
                            <div class="row">

                                <div class="col-md-4">
                                    <div class="form-group">
                                        <label>Old Registration Number<span class="text-danger">*</span></label>
                                        @Html.TextBoxFor(m => m.outerRegistrationNo, new { @class = "form-control", tabindex = 1 })
                                        @Html.ValidationMessageFor(m => m.outerRegistrationNo, "", new { @class = "text-danger" })
                                    </div>
                                </div>
                                <div class="col-md-4">
                                    <div class="form-group">
                                        <label>Old Certificate Number<span class="text-danger">*</span></label>
                                        @Html.TextBoxFor(m => m.outerCertificateNo, new { @class = "form-control", tabindex = 1 })
                                        @Html.ValidationMessageFor(m => m.outerCertificateNo, "", new { @class = "text-danger" })
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-md-12">
                    <div class="panel panel-default">

                        <div class="panel-heading">
                            <span class="title elipsis">
                                <h4>Details of Person In Charge</h4>
                            </span>
                        </div>
                        <div class="panel-body">
                            <div class="row">
                                <div class="col-md-3">
                                    <div class="form-group">
                                        <label>Name <span class="star">*</span></label>
                                        @Html.TextBoxFor(m => m.applicantName, new { @class = "form-control", tabindex = 1, @maxlength = "50" })
                                        @Html.ValidationMessageFor(m => m.applicantName, "", new { @class = "text-danger" })
                                    </div>
                                </div>
                                <div class="col-md-3">
                                    <div class="form-group">
                                        <label>Mobile No <span class="star">*</span></label>
                                        @Html.TextBoxFor(m => m.appMobileNo, new { maxlength = 10, @class = "form-control", tabindex = 2, onkeypress = "return isNumberKey(event);" })
                                        @Html.ValidationMessageFor(m => m.appMobileNo, "", new { @class = "text-danger" })
                                    </div>
                                </div>
                                <div class="col-md-3">
                                    <div class="form-group">
                                        <label>Email Id<span class="star">*</span></label>
                                        @Html.TextBoxFor(m => m.applicantEmailId, new { @class = "form-control", tabindex = 3, @maxlength = "250" })
                                        @Html.ValidationMessageFor(m => m.applicantEmailId, "", new { @class = "text-danger" })

                                    </div>
                                </div>
                                <div class="col-md-3">
                                    <div class="form-group">
                                        <label>Relevant Qualification <span class="star">*</span></label>
                                        @Html.TextBoxFor(m => m.qualification, new { @class = "form-control", tabindex = 4, @maxlength = "50" })
                                        @Html.ValidationMessageFor(m => m.qualification, "", new { @class = "text-danger" })
                                    </div>
                                </div>
                                <div class="clearfix"></div>
                                <div class="col-md-3">
                                    <div class="form-group">
                                        <label>Institution Name<span class="text-danger">*</span></label>
                                        @Html.TextBoxFor(m => m.institution, new { @class = "form-control", tabindex = "25", @maxlength = "250" })
                                        <div class="tool-tip  slideIn">Provide Name of Institute from where relevant qualification has been obtained.</div>
                                        @Html.ValidationMessageFor(m => m.institution, "", new { @class = "text-danger" })
                                    </div>
                                </div>
                                <div class="col-md-3">
                                    <div class="form-group">
                                        <label>Name Of Central/State Council</label>
                                        @Html.TextBoxFor(m => m.Central_StateCouncilName, new { @class = "form-control", tabindex = "26", @maxlength = "250" })
                                        <div class="tool-tip  slideIn">Provide Name of Council/State Medical Faculty from where registration has been done.</div>
                                        @Html.ValidationMessageFor(m => m.Central_StateCouncilName, "", new { @class = "text-danger" })
                                    </div>
                                </div>
                                <div class="col-md-3">

                                    <div class="form-group">
                                        <label>Registration Number(MCI/SMF) </label>
                                        @Html.TextBoxFor(m => m.registrationNumber, new { @class = "form-control", tabindex = 5, @maxlength = "20" })
                                        @Html.ValidationMessageFor(m => m.registrationNumber, "", new { @class = "text-danger" })
                                    </div>
                                </div>
                                <div class="col-md-3">
                                    <div class="form-group">
                                        <label>State<span class="star">*</span></label>
                                        @Html.DropDownListFor(m => m.applicantStateId, (IEnumerable<SelectListItem>)ViewBag.State1, "--Select--", new { @class = "form-control", tabindex = "6" })
                                        @Html.ValidationMessageFor(m => m.applicantStateId, "", new { @class = "text-danger" })
                                    </div>
                                </div>
                                <div class="clearfix"></div>
                                <div class="col-md-3">
                                    <div class="form-group">
                                        <label>District <span class="star">*</span></label>
                                        @Html.DropDownListFor(m => m.applicantDistrictId, (IEnumerable<SelectListItem>)ViewBag.District1, "--Select--", new { @class = "form-control", tabindex = "7" })
                                        @Html.ValidationMessageFor(m => m.applicantDistrictId, "", new { @class = "text-danger" })

                                    </div>
                                </div>
                                <div class="col-md-3">
                                    <div class="form-group">
                                        <label>Address <span class="star">*</span></label>
                                        @Html.TextAreaFor(m => m.applicantAddress, new { @class = "form-control", tabindex = 8, @maxlength = "250" })
                                        @Html.ValidationMessageFor(m => m.applicantAddress, "", new { @class = "text-danger" })
                                    </div>
                                </div>

                                <div class="col-md-3">
                                    <div class="form-group">
                                        <label>Pincode<span class="star">*</span></label>

                                        @Html.TextBoxFor(m => m.applicantPincode, new { @class = "form-control", tabindex = 9, maxlength = 6, onkeypress = "return isNumberKey(event);" })
                                        @Html.ValidationMessageFor(m => m.applicantPincode, "", new { @class = "text-danger" })
                                    </div>
                                </div>

                                <div class="clearfix"></div>
                            </div>

                        </div>

                    </div>
                </div>

            </div>
        }
        else if (Model.step == 1 || Model.stepValue == 1)
        {
            <div class="row">

                <div class="col-md-12">
                    <div class="panel panel-default">
                        <div class="panel-heading">
                            <span class="title elipsis">
                                <h4>Step 2</h4>
                            </span>
                        </div>
                    </div>
                </div>

                <div class="col-md-12">
                    <div class="panel panel-default">
                        <div class="panel-heading">
                            <span class="title elipsis">
                                <h4>Details of Medical Establishment</h4>
                            </span>
                        </div>
                        <div class="panel-body">
                            <div class="row">
                                <div class="col-md-4">
                                    <div class="form-group">
                                        <label>Area Of Establishment<span class="star">*</span></label><br>
                                        <div class="form-control">
                                            <label class="radio-inline">
                                                @Html.RadioButtonFor(m => m.establishmentArea, "Urban")
                                                Urban
                                            </label>
                                            <label class="radio-inline">
                                                @Html.RadioButtonFor(m => m.establishmentArea, "Rural")
                                                Rural
                                            </label>

                                        </div>
                                    </div>
                                    @Html.ValidationMessageFor(m => m.establishmentArea, "", new { @class = "text-danger" })

                                </div>
                                <div class="col-md-4">
                                    <div class="form-group">
                                        <label>Place Of Establishment<span class="star">*</span></label><br>
                                        <div class="form-control">
                                            <label class="radio-inline">
                                                @Html.RadioButtonFor(m => m.establishmentPlace, "Own")
                                                Own
                                            </label>
                                            <label class="radio-inline">
                                                @Html.RadioButtonFor(m => m.establishmentPlace, "Rental")
                                                Rental
                                            </label>
                                            <label class="radio-inline">
                                                @Html.RadioButtonFor(m => m.establishmentPlace, "Other")
                                                Other
                                            </label>

                                        </div>
                                    </div>
                                    @Html.ValidationMessageFor(m => m.establishmentPlace, "", new { @class = "text-danger" })

                                </div>

                                <div class="col-md-4 " id="divEstablishmentPlaceOther" style="display:@(Model.establishmentPlace == "Other" ? "" : "none");">
                                    <div class="form-group">
                                        <label>Please Specify (Place Of Establishment) <span class="text-danger">*</span></label>
                                        @Html.TextBoxFor(m => m.establishmentPlaceOther, new { @class = "form-control", tabindex = 5, @maxlength = 250 })
                                        @Html.ValidationMessageFor(m => m.establishmentPlaceOther, "", new { @class = "text-danger" })
                                    </div>
                                </div>


                                <div class="col-md-4">
                                    <div class="form-group">
                                        <label>Type Of Land<span class="star">*</span></label><br>
                                        <div class="form-control">
                                            <label class="radio-inline">
                                                @Html.RadioButtonFor(m => m.landType, "Commercial")
                                                Commercial
                                            </label>
                                            <label class="radio-inline">
                                                @Html.RadioButtonFor(m => m.landType, "Residential")
                                                Residential
                                            </label>
                                            <label class="radio-inline">
                                                @Html.RadioButtonFor(m => m.landType, "Mixed Population")
                                                Mixed Population
                                            </label>

                                        </div>
                                    </div>
                                    @Html.ValidationMessageFor(m => m.landType, "", new { @class = "text-danger" })

                                </div>
                                <div class="col-md-4">
                                    <div class="form-group">
                                        <label>Establishment Name<span class="text-danger">*</span></label>
                                        @Html.TextBoxFor(m => m.establishmentName, new { @class = "form-control", tabindex = 1, @maxlength = 250 })
                                        @Html.ValidationMessageFor(m => m.establishmentName, "", new { @class = "text-danger" })
                                    </div>
                                </div>
                                <div class="col-md-4">
                                    <div class="form-group">
                                        <label>Category <span class="text-danger">*</span></label>
                                        @Html.DropDownListFor(m => m.medicalEstablishmentId, (IEnumerable<SelectListItem>)ViewBag.MedicalEstablishment, "--Select--", new { @class = "form-control", tabindex = "2" })
                                        <div class="tool-tip  slideIn">Select Type of Medical Establishment that is to be registered </div>
                                        @Html.ValidationMessageFor(m => m.medicalEstablishmentId, "", new { @class = "text-danger" })
                                    </div>
                                </div>
                                <div class="col-md-4 OtherEstablishment" style="display: none;">
                                    <div class="form-group">
                                        <label>Please Specify (Category) <span class="text-danger">*</span></label>
                                        @Html.TextBoxFor(m => m.medicalEstablishmentOther, new { @class = "form-control", tabindex = 3, @maxlength = 25 })
                                        @Html.ValidationMessageFor(m => m.medicalEstablishmentOther, "", new { @class = "text-danger" })
                                    </div>
                                </div>


                                <div class="col-md-4">
                                    <div class="form-group">
                                        <label>Is operated by <span class="text-danger">*</span></label>
                                        @Html.DropDownListFor(m => m.operatedId, (IEnumerable<SelectListItem>)ViewBag.Operate, "--Select--", new { @class = "form-control", tabindex = "4" })

                                        @Html.ValidationMessageFor(m => m.operatedId, "", new { @class = "text-danger" })
                                    </div>
                                </div>
                                <div class="col-md-4 " id="divOperate" style="display: none;">
                                    <div class="form-group">
                                        <label id="lbloperate"></label>
                                        <span class="text-danger">*</span>
                                        @Html.TextBoxFor(m => m.operatedName, new { @class = "form-control", tabindex = 5, @maxlength = 250 })
                                        @Html.ValidationMessageFor(m => m.operatedName, "", new { @class = "text-danger" })
                                    </div>
                                </div>

                                <div class="col-md-3 clasElectricityBill" style="display: none;">
                                    <div class="form-group">
                                        <label>Upload Electricity Bill<span class="text-danger">*</span></label>
                                        <div class="fileUpload3">
                                            <span>Upload </span>
                                            @Html.TextBoxFor(m => m.ElectrycityBill, new { @type = "file", @class = "upload3", @onchange = "Fileupload(this)" })
                                            @Html.HiddenFor(m => m.ElectrycityBillPath)
                                        </div>
                                        @Html.ValidationMessageFor(m => m.ElectrycityBill, "", new { @class = "text-danger" })

                                    </div>
                                </div>
                                <div class="col-md-1 clasElectricityBill">
                                    <div class="form-group padding-top-30">
                                        <div style="float: left"><a id="ElectrycityBillAnchor" href="#" style="display: none;" target="_blank"><i class="fa fa-download fa-2x"></i></a></div>

                                    </div>
                                </div>









                                <div class="col-md-3 clasDivRegistry" style="display: none;">
                                    <div class="form-group">
                                        <label>Upload Registry<span class="text-danger">*</span></label>
                                        <div class="fileUpload3">
                                            <span>Upload </span>
                                            @Html.TextBoxFor(m => m.Registry, new { @type = "file", @class = "upload3", @onchange = "Fileupload(this)" })
                                            @Html.HiddenFor(m => m.RegistryPath)
                                        </div>
                                        @Html.ValidationMessageFor(m => m.Registry, "", new { @class = "text-danger" })
                                    </div>
                                </div>
                                <div class="col-md-1 clasDivRegistry">
                                    <div class="form-group padding-top-30">
                                        <div style="float: left"><a id="RegistryAnchor" href="#" style="display: none;" target="_blank"><i class="fa fa-download fa-2x"></i></a></div>
                                    </div>
                                </div>



                                <div class="col-md-3 clasRentalAgreement" style="display: none;">
                                    <div class="form-group">
                                        <label>Upload Rental Agreement<span class="text-danger">*</span></label>
                                        <div class="fileUpload3">
                                            <span>Upload </span>
                                            @Html.TextBoxFor(m => m.RentalAgreement, new { @type = "file", @class = "upload3", @onchange = "Fileupload(this)" })
                                            @Html.HiddenFor(m => m.RentalAgreementPath)
                                        </div>
                                        @Html.ValidationMessageFor(m => m.RentalAgreement, "", new { @class = "text-danger" })
                                    </div>
                                </div>
                                <div class="col-md-1 clasRentalAgreement">
                                    <div class="form-group padding-top-30">
                                        <div style="float: left"><a id="RentalAgreementAnchor" href="#" style="display: none;" target="_blank"><i class="fa fa-download fa-2x"></i></a></div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-md-12 firm" style="display: none">
                    <div class="panel panel-default">
                        <div class="panel-heading">
                            <span class="title elipsis">
                                <h4>Details of Partners</h4>
                            </span>
                        </div>
                        <div class="panel-body">
                            <div class="table-responsive">
                                <table class="table table-bordered css-serial tbUserOwner" id="tbleowner">
                                    <thead>
                                        <tr>
                                            <th width="5%" class="text-center">S.No.</th>
                                            <th width="20%">Name<span class="text-danger">*</span></th>
                                            <th width="20%">Age<span class="text-danger">*</span></th>
                                            <th width="20%">Fathers Name<span class="text-danger">*</span></th>
                                            <th width="20%">Mobile<span class="text-danger">*</span></th>
                                            <th width="20%">Email<span class="text-danger">*</span></th>
                                            <th width="20%">Address<span class="text-danger">*</span></th>
                                            <th width="15%">State<span class="text-danger">*</span></th>
                                            <th width="15%">District<span class="text-danger">*</span></th>
                                            <th width="15%">Pin Code<span class="text-danger">*</span></th>
                                            <th width="5%">Remove</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        @if (Model.step == 2 || Model.step == 3)
                                        {

                                            for (int i = 0; i < Model.NUHPartnerList.Count(); i++)
                                            {
                                                { z = i + 1; }   
                                            <tr>
                                                <td align="center"></td>
                                                <td>
                                                    <div class="form-group">

                                                        <input type="text" name="ownerName" id="ownerName" class="alpha" value="@Model.NUHPartnerList[i].ownerName" />
                                                    </div>
                                                </td>
                                                <td>
                                                    <div class="form-group">
                                                        <input type="text" name="ownerAge" id="ownerAge" value="@Model.NUHPartnerList[i].ownerAge"  onkeypress = "return isNumberKey(event);" maxlength="2" />

                                                    </div>
                                                </td>
                                                <td>
                                                    <div class="form-group">
                                                        <input type="text" name="ownerFatherName" id="ownerFatherName" class="alpha" value="@Model.NUHPartnerList[i].ownerFatherName" />

                                                    </div>
                                                </td>
                                                <td>
                                                    <div class="form-group on-focus">
                                                        <input type="text" name="ownerMobileNo" id="ownerMobileNo" value="@Model.NUHPartnerList[i].ownerMobileNo"  maxlength = "10", onkeypress = "return isNumberKey(event);"/>

                                                    </div>
                                                </td>
                                                <td>
                                                    <div class="form-group on-focus">
                                                        <input type="text" name="ownerEmailId" id="ownerEmailId" value="@Model.NUHPartnerList[i].ownerEmailId" />



                                                    </div>
                                                </td>
                                                <td>
                                                    <div class="form-group on-focus">
                                                        <input type="text" name="ownerAddress" id="ownerAddress" class="alpha" value="@Model.NUHPartnerList[i].ownerAddress" />



                                                    </div>
                                                </td>
                                                <td>
                                                    <div class="form-group">

                                                        @Html.DropDownListFor(modelItem => Model.NUHPartnerList[i].StateName, new SelectList(ViewBag.State1, "Value", "Text", Model.NUHPartnerList[i].StateName), "--Select--", new { @class = "form-control select", @id = "ownerStateId" + "_" + z, @Name = "ownerStateId", @onchange = "binddist2(this)", @data_value = z })
                                                    </div>
                                                </td>
                                                <td>
                                                    <div class="form-group">
                                                        @Html.DropDownListFor(modelItem => Model.NUHPartnerList[i].DistrictName, new SelectList(ViewBag.District1, "Value", "Text", Model.NUHPartnerList[i].DistrictName), "--Select--", new { @class = "form-control select", @id = "ownerDistrictId" + "_" + z, @Name = "ownerDistrictId", @readonly = "readonly" })

                                                    </div>
                                                </td>
                                                <td>
                                                    <div class="form-group on-focus">
                                                        <input type="text" name="ownerPincode" id="ownerPincode" value="@Model.NUHPartnerList[i].ownerPincode"/>


                                                    </div>
                                                </td>

                                                <td align="center">
                                                    <div class="padding-top-10">
                                                        <button id="btndelOwner" onclick="DeleteRowOwner(this);" type="button"><i class="fa fa-trash fa-2x text-warning"></i></button>
                                                    </div>
                                                </td>
                                            </tr>
                                                
                                            }
                                        }
                                        else
                                        {

                                            <tr>
                                                <td align="center"></td>
                                                <td>
                                                    <div class="form-group">

                                                        @Html.TextBoxFor(m => m.ownerName, "", new { @class = "alpha" })
                                                        @Html.ValidationMessageFor(m => m.ownerName, "", new { @class = "text-danger" })
                                                    </div>
                                                </td>
                                                <td>
                                                    <div class="form-group">
                                                        @Html.TextBoxFor(m => m.ownerAge, "{0:#.#}", new { @maxlength = "2", @onkeypress = "return isNumberKey(event);" })

                                                        @Html.ValidationMessageFor(m => m.ownerAge, "", new { @class = "text-danger" })
                                                    </div>
                                                </td>
                                                <td>
                                                    <div class="form-group">
                                                        @Html.TextBoxFor(m => m.ownerFatherName, "", new { @class = "alpha" })
                                                        @Html.ValidationMessageFor(m => m.ownerFatherName, "", new { @class = "text-danger" })
                                                    </div>
                                                </td>
                                                <td>
                                                    <div class="form-group on-focus">
                                                        @Html.TextBoxFor(m => m.ownerMobileNo, "", new { @maxlength = "10", @onkeypress = "return isNumberKey(event);" })
                                                        @Html.ValidationMessageFor(m => m.ownerMobileNo, "", new { @class = "text-danger" })
                                                    </div>
                                                </td>
                                                <td>
                                                    <div class="form-group on-focus">
                                                        @Html.TextBoxFor(m => m.ownerEmailId, "")
                                                        @Html.ValidationMessageFor(m => m.ownerEmailId, "", new { @class = "text-danger" })
                                                    </div>
                                                </td>
                                                <td>
                                                    <div class="form-group on-focus">
                                                        @Html.TextBoxFor(m => m.ownerAddress, "", new { @class = "alphaN" })
                                                        @Html.ValidationMessageFor(m => m.ownerAddress, "", new { @class = "text-danger" })
                                                    </div>
                                                </td>
                                                <td>
                                                    <div class="form-group">
                                                        @Html.DropDownListFor(m => m.ownerStateId, (IEnumerable<SelectListItem>)ViewBag.State1, "--Select--", new { @onchange = "binddist3(this)", @data_value = "1" })
                                                        @Html.ValidationMessageFor(m => m.ownerStateId, "", new { @class = "text-danger" })
                                                    </div>
                                                </td>
                                                <td>
                                                    <div class="form-group">
                                                        @Html.DropDownListFor(m => m.ownerDistrictId, (IEnumerable<SelectListItem>)ViewBag.District1, "--Select--", new { @id = "ownerDistrictId_1" })
                                                        @Html.ValidationMessageFor(m => m.ownerDistrictId, "", new { @class = "text-danger" })
                                                    </div>
                                                </td>
                                                <td>
                                                    <div class="form-group on-focus">
                                                        @Html.TextBoxFor(m => m.ownerPincode, "", new { @maxlength = "6", @onkeypress = "return isNumberKey(event);" })
                                                        @Html.ValidationMessageFor(m => m.ownerPincode, "", new { @class = "text-danger" })

                                                    </div>
                                                </td>

                                                <td align="center">
                                                    <div class="padding-top-10">
                                                        <button id="btndelOwner" onclick="DeleteRowOwner(this);" type="button"><i class="fa fa-trash fa-2x text-warning"></i></button>
                                                    </div>
                                                </td>
                                            </tr>
                                        }
                                    </tbody>
                                </table>
                            </div>
                            <div class="row">
                                <div class="col-md-4 col-md-offset-8">
                                    <button id="btnaddOwner" type="button" class="btn btn-info btn-md pull-right">Add More</button>
