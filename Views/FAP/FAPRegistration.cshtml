function isNumberKey(evt) {
    debugger;
    var charCode = (evt.which) ? evt.which : event.keyCode
    if (charCode > 31 && (charCode < 48 || charCode > 57))
        return false;
    return true;
}
function isalphabet(evt) {
    debugger;
    var charCode = (evt.which) ? evt.which : event.keyCode

    if (!(charCode >= 65 && charCode <= 120) && (charCode != 32 && charCode != 0))
        return false;
    return true;
}
