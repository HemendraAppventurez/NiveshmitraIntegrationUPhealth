@{
    ViewBag.Title = "Family Planning Dashboard";
    Layout = "~/Views/Shared/_UserLayout.cshtml";
}

<div class="content-title">
    <h3 class="main-title">
        <strong>असफल परिवार नियोजन डैशबोर्ड <span class="small">Unsuccessful Family Planning Dashboard</span></strong>
    </h3>
</div>

<div id="content" class="dashboard padding-90 margin-bottom-50">
    <div class="row">
        <div class="col-md-12">
            <ul class="circle">
                <li class="col-md-2 col-sm-6 col-xs-12"></li>
                <li class="col-md-4 col-sm-6 col-xs-12">
                    <a href="@Url.Action("PrintApplicationForm", "FAP")" class="red" target="_blank"> 
                         <img src="@Url.Content("~/images/print_icon.png")"><br />
                         Print Your Submitted Application
                    </a>
                </li>
                <li class="col-md-4 col-sm-6 col-xs-12">
                    <a href="@Url.Action("ViewFAPList", "FAP")" class="indigo"> 
                        <img src="@Url.Content("~/images/view_icon.png")"><br />
                        View Submitted Application Status
                    </a>
                </li>
            </ul>
        </div>
    </div>
</div>

<script>

    $(document).ready(function () {
        if ('@TempData["Message"]' != '') {
            swal("", '@TempData["Message"]', 'success');
        }
    });

</script>