@model CCSHealthFamilyWelfareDept.Models.IMCModel

@if (Model != null)
{
    <div class="col-md-12">
        <div class="panel panel-default">
            <div class="panel-heading">
                <span class="title elipsis">
                    <h4>Application Details</h4>
                </span>
            </div>
            <div class="panel-body">
                <table class="table-user-information table table-bordered table-striped" style="margin-bottom: 0;">
                    <tr>
                        <td style="width: 16%"><strong>Application No.</strong></td>
                        <td>@Model.registrationNo</td>
                        <td style="width: 15%"><strong>Application Date</strong></td>
                        <td>@Model.requestDate</td>
                        <td style="width: 15%"><strong>Status</strong></td>
                        <td style="width: 20%">@Model.appliedStatus</td>
                    </tr>
                    <tr>
                        <td><strong>Full Name</strong></td>
                        <td>@Model.fullName</td>
                        <td><strong>Mobile </strong></td>
                        <td>@Model.mobileNo</td>
                        <td></td>
                        <td></td>
                        @*<td><strong>Passport No.</strong></td>
                        <td>@Model.passportNo</td>*@
                    </tr>
                </table>
            </div>
        </div>
    </div>
}
