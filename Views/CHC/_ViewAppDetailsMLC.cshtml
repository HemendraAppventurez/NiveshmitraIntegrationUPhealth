@model CCSHealthFamilyWelfareDept.Models.MLCModel

<style>
    .form-group label
    {
        color: #333;
        font-weight: 600;
    }
</style>
<div class="col-md-12">
    <div class="panel panel-default">
        <div class="panel-heading">
            <span class="title elipsis">
                <h4>Medico - Legal Certificate Details</h4>
            </span>
        </div>
        <div class="panel-body">
            <div class="row">
                <div class="form-group">
                    <div class="col-md-3">
                        <label>Registration No.</label>
                    </div>
                    <div class="col-md-3">@Model.registrationNo</div>
                    <div class="col-md-3">
                        <label>Brought By Name</label>
                    </div>
                    <div class="col-md-3">@Model.fullName</div>
                    <div class="clearfix"></div>
                </div>
                <div class="clearfix"></div>
                <div class="form-group">
                    <div class="col-md-3">
                        <label>Patient Brought By</label>
                    </div>
                    <div class="col-md-3">@Model.patientBroughtBy</div>
                    @if (Model.patientBroughtBy == "Relative")
                    {
                        <div class="col-md-3">
                            <label>Relation With Patient</label>
                        </div>
                        <div class="col-md-3">
                            @Model.broughtByPersonrelation

                        </div>
                    }
                    @if (Model.patientBroughtBy == "Police")
                    {
                        <div class="col-md-3">
                            <label>Police Id</label>
                        </div>
                        <div class="col-md-3">
                            @Model.idNo
                        </div>
                    }
                    <div class="clearfix"></div>
                </div>
                @if (Model.patientBroughtBy != "Police")
                {
                    <div class="clearfix"></div>
                     <div class="form-group">
                    <div class="col-md-3">
                        <label>Id Type</label>
                    </div>
                    @if (Model.idTypeName != null)
                    {
                        <div class="col-md-3">
                            @Model.idTypeName
                        </div>
                    }
                    else
                    {
                        <div class="col-md-3">
                            <label>NaN</label>
                        </div>
                    }
                    <div class="col-md-3">
                        <label>Id Number</label>
                    </div>
                    @if (Model.idNumber != "")
                    {
                        <div class="col-md-3">
                            @Model.idNumber
                        </div>
                    }
                    else
                    {
                        <div class="col-md-3">
                            <label>NaN</label>
                        </div>
                    }
                    <div class="clearfix"></div>
                </div>
                }
                <div class="clearfix"></div>
                <div class="form-group">
                    <div class="col-md-3">
                        <label>Mobile No</label>
                    </div>
                    <div class="col-md-3">
                        @Model.mobileNo
                    </div>
                    <div class="col-md-3">
                        <label>Email Id</label>
                    </div>
                    <div class="col-md-3">
                        @Model.emailId
                    </div>
                    <div class="clearfix"></div>
                </div>
                <div class="clearfix"></div>
                <div class="form-group">
                    <div class="col-md-3">
                        <label>Designation</label>
                    </div>
                     @if (Model.broughtByDesignation != "")
                     {
                    <div class="col-md-3">
                        @Model.broughtByDesignation
                    </div>
                     }
                     else
                     {
                         <div class="col-md-3">
                      <label>NaN</label>
                    </div>
                     }
                    <div class="col-md-3">
                        <label>Address</label>
                    </div>
                    <div class="col-md-3">
                        @Model.broughtByaddress
                    </div>
                   
                    <div class="clearfix"></div>
                </div>
                <div class="clearfix"></div>
                <div class="form-group">
                     <div class="col-md-3">
                        <label>State</label>
                    </div>
                    <div class="col-md-3">
                        @Model.broughtBystateName
                    </div>
                    <div class="col-md-3">
                        <label>District</label>
                    </div>
                    <div class="col-md-3">
                        @Model.broughtBydistrictName
                    </div>
                  
                    <div class="clearfix"></div>
                </div>
                 <div class="clearfix"></div>
                <div class="form-group">
                    
                    <div class="col-md-3">
                        <label>Pin Code</label>
                    </div>
                    <div class="col-md-3">
                        @Model.pinCode
                    </div>
                    <div class="clearfix"></div>
                </div>
                <div class="clearfix"></div>
            </div>
        </div>
    </div>
</div>

<div class="col-md-12">
    <div class="panel panel-default">
        <div class="panel-heading">
            <span class="title elipsis">
                <h4>Patient Detail</h4>
            </span>
        </div>
        <div class="panel-body">
            <div class="row">
                <div class="form-group">
                    <div class="col-md-3">
                        <label>Patient Name</label>
                    </div>
                    <div class="col-md-3">
                        @Model.patientName
                    </div>
                    <div class="col-md-3">
                        <label>Age</label>
                    </div>
                   @if (Model.age != null)
                {
                    <div class="col-md-3">
                        @Model.age

                    </div>}
                else
                {
                    <div class="col-md-3">
                        <label>NaN</label>
                    </div>}
                
                    <div class="clearfix"></div>
                </div>
                <div class="clearfix"></div>
                <div class="form-group">
                    <div class="col-md-3">
                        <label>Patient Gender</label>
                    </div>
                    <div class="col-md-3">
                        @Model.patientGender
                    </div>
                    <div class="col-md-3">
                        <label>Occupation</label>
                    </div>
                     @if (Model.occupation != "")
                {
                <div class="col-md-3">
                    @Model.occupation
                </div>
                }
                else
                {
                    <div class="col-md-3">
                   <label>NaN</label>
                </div>
                }
                    <div class="clearfix"></div>
                </div>
                <div class="clearfix"></div>
                <div class="form-group">
                    <div class="col-md-3">
                        <label>Address</label>
                    </div>
                    <div class="col-md-3">
                        @Model.address
                    </div>
                    <div class="col-md-3">
                        <label>District</label>
                    </div>
                    <div class="col-md-3">
                        @Model.DistrictName
                    </div>
                    <div class="clearfix"></div>
                </div>
                <div class="clearfix"></div>
                <div class="form-group">
                    <div class="col-md-3">
                        <label>Tehsil</label>
                    </div>
                    <div class="col-md-3">
                        @Model.tehsilName
                    </div>
                    <div class="col-md-3">
                        <label>Area/Road</label>
                    </div>
                    <div class="col-md-3">
                        @Model.areaRoadName
                    </div>

                    <div class="clearfix"></div>
                </div>
                <div class="clearfix"></div>
                <div class="form-group">
                    <div class="col-md-3">
                        <label>Police Station</label>
                    </div>
                    <div class="col-md-3">
                        @Model.policeStation
                    </div>
                    <div class="col-md-3">
                        <label>Details</label>
                    </div>
                    <div class="col-md-3">
                        @Model.details
                    </div>

                    <div class="clearfix"></div>
                </div>
                <div class="clearfix"></div>
            </div>

        </div>
    </div>
</div>

<div class="col-md-12">
    <div class="panel panel-default">
        <div class="panel-heading">
            <span class="title elipsis">
                <h4>Selected Health Unit For Appointment</h4>
            </span>
        </div>
        <div class="panel-body">
            <div class="row">
                <div class="form-group">
                    <div class="col-md-3">
                        <label>Health Unit</label>
                    </div>
                    <div class="col-md-3">
                        @Model.forwardtypeName
                    </div>
                    <div class="col-md-3">
                        <label>District</label>
                    </div>
                    <div class="col-md-3">
                        @Model.healthUnitDistrictName
                    </div>
                    <div class="clearfix"></div>
                </div>
                <div class="clearfix"></div>
                <div class="form-group">
                    <div class="col-md-3">
                        <label>Area</label>
                    </div>
                    <div class="col-md-3">
                        @Model.forwardtoName
                    </div>

                    <div class="col-md-3">
                        <label>&nbsp;</label>
                    </div>
                    <div class="col-md-3">
                        &nbsp;
                    </div>
                    <div class="clearfix"></div>
                </div>
                <div class="clear-fix"></div> 
            </div> 
        </div>
    </div>
</div> 
<div class="col-md-12">
    <div class="panel panel-default">
        <div class="panel-heading">
            <span class="title elipsis">
                <h4>Injury Detail</h4>
            </span>
        </div>
        <div class="panel-body">
            <div class="row">
                <div class="form-group">
                    <div class="col-md-3">
                        <label>Doctor Name</label>
                    </div>
                    <div class="col-md-3">
                        @Model.doctorName
                    </div>
                    <div class="col-md-3">
                        <label>Designation</label>
                    </div>
                    <div class="col-md-3">
                        @Model.Designation
                    </div>
                    <div class="clearfix"></div>
                </div>
                <div class="clearfix"></div>
               
            </div> 
        </div>
    </div>
</div> 

<div class="col-md-12">
    <div class="panel panel-default">
        <div class="panel-heading">
            <span class="title elipsis">
                <h4>Enquiry Detail</h4>
            </span>
        </div>
        <div class="panel-body">
            <div class="row">
                 
              <div id="childlist">
       @Html.Action("BindCHCChildList", "CHC", new { registrationNo = @Model.registrationNo })
                </div>
               
            </div> 
        </div>
    </div>
</div> 
 
<div class="clearfix"></div>


