<script type="text/javascript">

    function OpenViewStatusModel(RegistrationID) {
        debugger

        $("input[type=hidden][name=hiddentextID]").val(RegistrationID);
        $.ajax({
            url: '@Url.Action("GetViewApplicationStatusNUH", "Report")',
            data: { registrationID: RegistrationID },
            type: 'GET',
            //dataType: 'HTML',
            contentType: "application/json; charset=utf-8",
        })
        .success(function (returndata) {
            debugger
            if (returndata != "") {
                $.each(returndata, function () {

                    $("#ApplicationDateSpanID").text(this.AppSubmittedDate);
                    $("#AppSubmittedBy").text(this.AppSubmittedBy);


                });

                $("#btnShowPopup").click();
                setTimeout(function () {
                    $('.modal-body').scrollTop(0);
                }, 300);

            }
            // $("#dvLoader").hide();
        })
        .error(function (xhr, status) {
            // $('#DIV-ApplicationList').html('');
            swal("", 'Details Not Found !', "warning");
            $("#dvLoader").hide();
        });

    }
</script>
