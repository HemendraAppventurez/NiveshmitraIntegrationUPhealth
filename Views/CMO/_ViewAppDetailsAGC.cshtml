@model CCSHealthFamilyWelfareDept.Models.AGCModel

@if(Model!=null&& Model.regisIdAGC>0)
{

<div class="col-md-12">
    <div class="panel panel-default">
        <div class="panel-heading">
            <span class="title elipsis">
                <h4>Applicant Type</h4>
            </span>
        </div>
        <div class="panel-body">
            <div class="row">
                <div class="form-group">
                    <div class="col-md-3">
                        <label>Registration No.</label>
                    </div>
                    <div class="col-md-3">@Model.registrationNo</div>
                    <div class="col-md-3">
                        <label>Application Type</label>
                    </div>
                    <div class="col-md-3">@Model.applicantTypeName</div>
                    <div class="clearfix"></div>
                </div>
                <div class="clearfix"></div>
                <div class="form-group">
                    <div class="col-md-3">
                        <label>Application Sub Type</label>
                    </div>
                    <div class="col-md-3">@Model.applicantSubTypeName</div>
                    <div class="col-md-3">
                        <label>Order Detail</label>
                    </div>
                    <div class="col-md-3">@Model.orderDetail</div>
                    <div class="clearfix"></div>
                </div>
               
                <div class="clearfix"></div>
            </div>
        </div>
    </div>
</div>

<div class="col-md-12">
    <div class="panel panel-default">
        <div class="panel-heading">
            <span class="title elipsis">
                <h4>Brought By Person Detail</h4>

            </span>
        </div>
        <div class="panel-body">
            <div class="row">
                <div class="form-group">
                    <div class="col-md-3">
                        <label>Brought By Persone Name</label>
                    </div>
                    <div class="col-md-3">@Model.fullName</div>
                    <div class="col-md-3">
                        <label>Mobile Number</label>
                    </div>
                    <div class="col-md-3">@Model.mobileNo</div>
                    <div class="clearfix"></div>
                </div>
                <div class="clearfix"></div>
                <div class="form-group">
                    <div class="col-md-3">
                        <label>Email</label>
                    </div>
                    <div class="col-md-3">@Model.emailId</div>
                    <div class="col-md-3">
                        <label>Gender</label>
                    </div>
                    <div class="col-md-3">@Model.gender</div>
                    <div class="clearfix"></div>
                </div>
                <div class="clearfix"></div>
                <div class="form-group">
                    <div class="col-md-3">
                        <label>Address</label>
                    </div>
                    <div class="col-md-3">@Model.address </div>
                    <div class="col-md-3">
                        <label>State</label>
                    </div>
                    <div class="col-md-3">@Model.StateName </div>
                    <div class="clearfix"></div>
                </div>
                
                <div class="clearfix"></div>
                 <div class="form-group">
                    <div class="col-md-3">
                        <label>District</label>
                    </div>
                    <div class="col-md-3">@Model.DistrictName </div>
                    <div class="col-md-3">
                        <label>Pin Code</label>
                    </div>
                    <div class="col-md-3">@Model.pinCode </div>
                    <div class="clearfix"></div>
                </div>
                <div class="clearfix"></div>
                 <div class="form-group">
                    <div class="col-md-3">
                        <label>Id Type</label>
                    </div>
                    <div class="col-md-3">@Model.idTypeName</div>
                    <div class="col-md-3">
                        <label>Id Number</label>
                    </div>
                    <div class="col-md-3">@Model.idNumber </div>
                    <div class="clearfix"></div>
                </div>
                <div class="clearfix"></div>

            </div>

        </div>
    </div>
</div>



<div class="col-md-12">
    <div class="panel panel-default">
        <div class="panel-heading">
            <span class="title elipsis">
                <h4>Suspected Person Detail</h4>
            </span>
        </div>
        <div class="panel-body">
            <div class="row">
                <div class="form-group">
                    <div class="col-md-3">
                        <label>Suspected Person Name</label>
                    </div>
                    <div class="col-md-3">@Model.susName </div>
                    <div class="col-md-3">
                        <label>Suspected Person Father's Name</label>
                    </div>
                    <div class="col-md-3">@Model.susFatherName </div>
                    <div class="clearfix"></div>
                </div>
                <div class="clearfix"></div>
                <div class="form-group">
                    <div class="col-md-3">
                        <label>Suspected Person Mother's Name</label>
                    </div>
                    <div class="col-md-3">@Model.susMotherName</div>
                    <div class="col-md-3">
                        <label>Suspected Person Mobile Number</label>
                    </div>
                    <div class="col-md-3">@Model.susMobileNo</div>
                    <div class="clearfix"></div>
                </div>
              <div class="clearfix"></div>
                <div class="form-group">
                    <div class="col-md-3">
                        <label>Suspected Person Email</label>
                    </div>
                    <div class="col-md-3">@Model.susEmail </div>
                    <div class="col-md-3">
                        <label>Appointment Date</label>
                    </div>
                    <div class="col-md-3">@Model.appointmentDate</div>
                    <div class="clearfix"></div>
                </div>
              
            </div>

        </div>
    </div>
</div>

<div class="clearfix"></div>

}
else
{

<label>NO Record </label>
}