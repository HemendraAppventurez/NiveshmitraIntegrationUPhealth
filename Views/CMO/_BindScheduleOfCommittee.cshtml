@model List<CCSHealthFamilyWelfareDept.Models.NUHDetailsModel>

@if (Model != null && Model.Count() > 0)
{
<div class="col-md-12 text-right margin-top-25">
    <div class="row">
        <label>Total Engagement : @Model.Count()</label>
    </div>
</div>
<table class="table table-bordered">
    <thead>
        <tr class="webgrid-header">
            <th valign="top">Sl. No. </th>
            <th valign="top">Inspection Date</th>
            <th valign="top">Application No</th> 
            <th valign="top">Medical Establishment</th>
            <th valign="top">Establishment Name</th>
            <th valign="top">Address Proof</th>
            <th valign="top">UPCI/SME No</th>
            <th width="4%" valign="center">Affidavit</th>
        </tr>
    </thead>
    <tbody>
        @{
    int slNo = 1;
                    }
        @foreach(var item in Model)
        {
           
        <tr>
            <td class="white">@slNo</td>
            <td>@item.inspectionDate</td>
            <td>@item.registrationNo</td>
            <td>@item.medicalEstablishment</td>
            <td>@item.establishmentName</td>
            <td>
                @if (@item.addressproofFilePath == null || @item.addressproofFilePath == "")
                {
                    <label></label>
                }else
                {<a href="@Url.Content(item.addressproofFilePath)" class="active none" target="_blank"><i class="big fa fa-download" aria-hidden="true"></i></a>}</td>
            <td>@item.UPMCI_SMF_Number</td>
            <td align="center"><a href="@Url.Content(item.notarizedAffidavitFilePath)" class="active none" target="_blank"><i class="big fa fa-download" aria-hidden="true"></i></a></td>
        </tr>
            slNo++;
        } 
    </tbody>
</table>
}
else
{
    <div class="row">
        <div class="col-md-12">
            <span class="text-danger form-control" style="font-weight:600;font-size:18px;">No record found !</span>
        </div>
    </div>
}
