        <div class="panel-heading">
            <span class="title elipsis">
                <h4>Type of Facilities Offered</h4>
            </span>
        </div>
        <div class="panel-body">
            <div class="row">
                @if (Model.isInPatient == true)
                {
                    <div class="form-group">

                        <div class="col-md-3">
                            <label>InPatient</label>
                        </div>
                        <div class="col-md-3">
                            <label>Yes</label>
                        </div>
                        <div class="col-md-3">
                            <label>No. of Bed</label>
