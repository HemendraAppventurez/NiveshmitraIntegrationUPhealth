                        <div class="clearfix"></div>
                    </div>
                
                }
                else
                {
                    <div class="form-group">

                        <div class="col-md-3">
                            <label>InPatient</label>
                        </div>
                        <div class="col-md-3">
                            <label>No</label>
                        </div>
                        <div class="clearfix"></div>
                    </div>
                }
