                        <div class="clearfix"></div>
                    </div>
                    <div class="form-group">
                        <div class="col-md-3">
                            <label>OutPatient type </label>
                        </div>
                        <div class="col-md-6">
                            @foreach (var v in ViewBag.VBoutpateint)
                            {                    
              
                                <span>@v.outPatientName
                                    @if (io > 1)
                                    { 
                                        <span>,</span>
                                    }

                                </span>
                                    io--;
                            }
                        </div>
                    </div>
                }
                else
                {
                    <div class="form-group">
                        <div class="col-md-3">
                            <label>OutPatient</label>
                        </div>
                        <div class="col-md-3">
