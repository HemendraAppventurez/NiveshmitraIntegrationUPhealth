                            <h4 id="hScheComm"></h4>
                        </span>
                    </div>
                    <div id="divScheduledInspection" class="panel-body">
                    </div>
                </div>
            </div> 
            }
            else if (Model.appStatus == 2)
            {
                if (ViewBag.validRequest != null && ViewBag.validRequest == true)
                {
            <div class="col-md-12">
