    }
}

$("input[name=forwardtype]").click(function () {
    binddropdown($("input[name=forwardtype]:checked").val());
    $('#forwardedType').val($("input[name=forwardtype]:checked").val());
});
function binddropdown(Id) {

    debugger;
    $.ajax({
        url: '@Url.Action("BindForwardDropdownlist", "CMO")',
        data: { rollId: Id },
        type: 'GET',
        async: false,
        dataType: 'json',
        success: function (data) {
            $("#forwardedTo").empty();
            $("#forwardedTo").append('<option value="">--Select--</option>');
            $.each(data, function (id, result) {
                $("#forwardedTo").append('<option value="' + result.Value + '">' + result.Text + '</option>');
            })
        },
        error: function (xhr, status) {
            //alert(status);
        }
    });
}
function acceptApp() {
    debugger;
    swal({
        title: "Are you sure you want to Accept this Application?",
        text: "Once you Accept you will not be able to change it.",
        type: "warning",
        showCancelButton: true,
        confirmButtonClass: "btn-danger",
        confirmButtonText: "Yes, Accept it !",
        cancelButtonText: "No, Cancel it !",
        closeOnConfirm: true,
        closeOnCancel: true
    }, function (isConfirm) {
        if (isConfirm) {
            $('#bitAppAccept').click();
        }
    });
}

</script>
