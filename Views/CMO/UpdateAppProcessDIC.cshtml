}

function CheckCommMemberSelected() {
    $("#spanCommMember").hide();
    if ($("input[name='chkCommMember']:checked").length == 0) {
        $("#spanCommMember").show();
        return false;
    }
    else {
        return true;
    }
}

function CheckCommMemberSelected1() {
    $("#spanDisbilityPer1").hide();
    $("#spanCommMember1").hide();
    var result = true;
    if (!$("input[name='rblDisbilityPer']:checked").val()) {
        $("#spanDisbilityPer1").show();
        result = false;
    }
    if ($("input[name='chkCommMember']:checked").length == 0) {
        $("#spanCommMember1").show();
        result = false;
    }
    return result;
}

$("input[name='rblReportType']").change(function () {
    if ($(this).val() == "2") {
        $('#divNextButton').hide();
        $('#divInsRejectRemarks').show();
