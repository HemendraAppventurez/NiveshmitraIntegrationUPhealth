@model CCSHealthFamilyWelfareDept.Models.DICAppProcessModel
@{
    ViewBag.Title = "UpdateAppProcessDIC";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

@{
    string actionName = "";
    actionName = Model.appStatus < 2 ? "ReceivedApplicationDIC" : "InProcessApplicationDIC";
}
<style>
    .form-group label {
        color: #333;
        font-weight: 600;
    }
</style>
<div class="content-title">
    <h3 class="main-title">
        @if (Model.appStatus <= 1)
        {
            <div class="col-md-12 text-left">विकलांगता प्रमाणपत्र के निर्गमन का कुल लंबित आवेदन<span class="small display-block">Total Pending Application of  Issuance of Disability Certificate</span></div>
        }
        else
        {
            <div class="col-md-12 text-left">विकलांगता प्रमाणपत्र के निर्गमन का प्रक्रिया आवेदन<span class="small display-block">In-Process Application Request for the Issuance of Disability Certificate</span></div>
        }
        @*<a href="@Url.Action(actionName, "CMO")" class="btn btn-info btn-sm pull-right">Back</a>*@
    </h3>
</div>
<div id="content" class="dashboard padding-20 margin-bottom-50">
    <div class="row">
        <div id="divAppDetails">
            @Html.Action("DICAppDetails", "CMO", new { regisId = Model.regisIdDIC })
        </div>
        <div class="clearfix"></div>
        @using (Html.BeginForm("UpdateAppProcessDIC", "CMO", FormMethod.Post, new { @id = "frmUpdateAppProcessDIC" }))
        {
            @Html.AntiForgeryToken()
            @Html.HiddenFor(m => m.regisIdDIC)
            @Html.HiddenFor(m => m.appStatus)
            if (Model.appStatus == -1)
            {
            <div class="col-md-12">
                <div class="panel-body process">
                    <ul class="breadcrumb">
                        <li class="current"><a href="javascript:;" class="none">Step 1</a></li>
                        <li class="active"><a href="javascript:;" class="none">Step 2</a></li>
                    </ul>
                </div>
            </div>
            <div id="divScrutiApplication" class="col-md-12">
                <div class="panel panel-default">
                    <div class="panel-heading">
                        <span class="title elipsis">
                            <h4>Scrutinized & Accept or Reject The Application</h4>
                        </span>
                    </div>
                    <div class="panel-body">
                        <div class="row">
                            <div class="col-md-3">
                                <div class="form-group">
                                    <label class="margin-top-10 bold">View the Application Form</label>
                                </div>
                            </div>
                            <div class="col-md-2">
                                <button type="button" id="btnViewAppForm" class="btn btn-block btn-info none" style="width: 250px">View Application</button>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
                                
            <div id="divViewAppDetails" style="display: none;">
                @Html.Action("ViewAppDetailsDIC", "CMO", new { regisId = Model.regisIdDIC })
                <div class="col-md-12">
                    <div class="panel-body" id="divBtnAppAcceptReject">
                        <div class="form-group">
                            <div class="col-md-2 col-md-offset-4">
                                <button name="button" type="button" value="appAccept" onclick="return acceptApp();" class="btn btn-block btn-primary">Accept</button>
                                <button id="bitAppAccept" name="button" type="submit" value="appAccept" style="display: none;" class="btn btn-block btn-primary btnAccept">Accept</button>
                            </div>
                            <div class="col-md-2">
                                <button id="btnAppReject" type="button" class="btn btn-block btn-danger reject-rsn">Reject</button>
                            </div>
                        </div>
                    </div>
                    <div class="panel panel-default" id="divAppRejectRemarks" style="display: none;">
                        <div class="panel-heading">
                            <span class="title elipsis">
                                <h4>Reject</h4>
                            </span>
                        </div>
                        <div class="panel-body">
                            <div class="row">
                                <div class="form-group">
                                    <div class="col-md-5">
                                        <label><b>Remarks <span class="text-danger">*</span></b> </label>
                                        @Html.TextAreaFor(m => m.rejectedRemarks, new { @class = "form-control" })
                                        @Html.ValidationMessageFor(m => m.rejectedRemarks)
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div id="divBtnSubmitCancel" class="panel-footer text-center" style="display: none;">
                            <button type="submit" class="btn btn-primary">Submit</button>&nbsp;&nbsp;&nbsp;
                            <button id="btnAppRejectCancel" type="button" class="btn btn-danger reject-rsn">Cancel</button>
                        </div>
                    </div>
                </div>
            </div> 
