@model CCSHealthFamilyWelfareDept.Models.ProcessType
@{
    ViewBag.Title = "AppliedApplicationMER";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
@using CCSHealthFamilyWelfareDept.Models
@{
    SessionManager SM = new SessionManager();
    PermissionModel objPerm = new PermissionModel();
    objPerm = PermissionLinks.LinkPermission(Session["UserPermission"] as List<PermissionModel>, Convert.ToInt32(AbbrServiceName.MER));
}
<div class="content-title">
    <h3 class="main-title">
        <div class="col-md-12 text-left">चिकित्सा प्रतिपूर्ति का पंजीकरण<span class="small display-block">Request For Payment of Medical Reimbursement</span></div>
    </h3>
</div>

<div id="content" class="dashboard padding-20 margin-bottom-50">

    @if (objPerm.IsSrvProcessPer || SM.RollID == 2 || SM.RollID == 5)
    {
        <h4 style="border-bottom: 1px solid #ccc; padding-bottom: 10px;">Application Process</h4>

        <div class="row">
            <div class="col-md-12">
                <ul class="circle">
                    <li class="col-md-3 col-sm-6 col-xs-12">
                        <a href="@Url.Action("ReceivedApplicationMER", "CMO")" class="red">
                            <img src="@Url.Content("~/images/womens-health.png")">
                            1. कुल लंबित आवेदन
                      <span class="nmicon2">Total Pending Application</span>
                            <span class="nmicon badge">@Model.totalAppReceived</span> </a></li>
                    <li class="col-md-3 col-sm-6 col-xs-12">
                        <a href="@Url.Action("InProcessApplicationMER", "CMO")" class="red">
                            <img src="@Url.Content("~/images/womens-health.png")">
                            2. आवेदन प्रक्रिया में है
                      <span class="nmicon2">In-Process Application </span>
                            <span class="nmicon badge">@Model.totalAppInProcess</span> </a></li>
                    <li class="col-md-3 col-sm-6 col-xs-12">
                        <a href="@Url.Action("ApprovedApplicationMER", "CMO")" class="indigo">
                            <img src="@Url.Content("~/images/immunizations.png")">
                            3. स्वीकृत आवेदन 
                      <span class="nmicon2">Approved Application </span>
                            <span class="nmicon badge">@Model.totalAppApproved</span> </a></li>
                    <li class="col-md-3 col-sm-6 col-xs-12">
                        <a href="@Url.Action("RejectedApplicationMER", "CMO")" class="cyan">
                            <img src="@Url.Content("~/images/registration.png")">
                            4. अस्वीकृत आवेदन 
                      <span class="nmicon2">Rejected Application </span>
                            <span class="nmicon badge">@Model.totalAppRejected</span> </a></li>
                </ul>
            </div>
        </div>
        <br />
    }
    @if (objPerm.IsSrvApplyPer || SM.RollID == 2 || SM.RollID == 5)
    {
        <h4 style="border-bottom: 1px solid #ccc; padding-bottom: 10px;">Application Form - Counter Registration</h4>

        <div class="row">
            <div class="col-md-12">
                <ul class="circle">
                    <li class="col-md-3 col-sm-6 col-xs-12">
                        <a href="@Url.Action("MERRegistration", "AppRegistration")" class="red">
                            <img src="@Url.Content("~/images/womens-health.png")">
                            New Application </a></li>
                    <li class="col-md-3 col-sm-6 col-xs-12">
                        <a href="@Url.Action("ViewMERInComplete", "AppRegistration")" class="red">
                            <img src="@Url.Content("~/images/womens-health.png")">
                            Incomplete Application </a></li>
                    <li class="col-md-3 col-sm-6 col-xs-12">
                        <a href="@Url.Action("ViewMERComplete", "AppRegistration")" class="indigo">
                            <img src="@Url.Content("~/images/immunizations.png")">
                            Complete Application </a></li>
                </ul>
            </div>
        </div>
    }
    @if (objPerm.IsSrvReportPer || SM.RollID == 2 || SM.RollID == 5)
    {
    }
</div>
<div class="clearfix"></div>
